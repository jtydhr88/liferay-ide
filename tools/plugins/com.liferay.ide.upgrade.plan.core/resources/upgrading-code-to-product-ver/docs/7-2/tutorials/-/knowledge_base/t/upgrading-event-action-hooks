				visible: false,
				zIndex: Liferay.zIndex.TOOLTIP
			}
		);

		Liferay.on(
			'beforeNavigate',
			function(event) {
				Liferay.Data.LFR_PORTAL_TOOLTIP.getTooltip().hide();
			}
